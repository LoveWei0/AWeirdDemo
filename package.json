{
  "name": "xxx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fix": "eslint src --fix",
    "format": "prettier --write \"./**/*.{html,react,ts,js,json,md}\"",
    "lint:eslint": "eslint src/**/*.{ts,react} --cache --fix",
    "lint:style": "stylelint src/**/*.{css,less,scss,react,postcss} --cache --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,react}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
